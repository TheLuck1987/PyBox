@if (Title != null && FormContent != null && FormObject != null)
{
    <Modal TitleIcon=@TitleIcon  Title=@Title ModalBody=@FormContent FormValidator=@FormValidator ConfirmLable=@ConfirmLable ConfirmCssColor=@ConfirmColor FormObject=@FormObject
           FormAlertClass=@FormAlertClass FormAlertMessage=@FormAlertMessage HideCancelButton=false OnExit=@OnExit ModalIsForm=true />
}